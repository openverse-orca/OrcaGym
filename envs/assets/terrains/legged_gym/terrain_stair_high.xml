        <texture type="2d" name="Plastic010_normal"     file="../../textures/Plastic010_2K-PNG/Plastic010_2K-PNG_NormalDX.png"/>

        <material name="pbr_plastic_gray" metallic ="0.05" roughness="1.0" rgba="1 0 0 1.0">
        <rgb        texture="Plastic010_color"/>
        <normal     texture="Plastic010_normal"/>
        <roughness  texture="Plastic010_roughness"/>      
        </material>
    </asset>
    <worldbody>
        <body name="terrain" pos="-30.0 0.0 0.0">
            <geom type="box" pos="0.0 0.0 0.0" size="4.0 4.0 0.1"  material="pbr_plastic_gray"/>
            <geom type="box" pos="0.0 0.0 0.0" size="3.8 3.8 0.2"  material="pbr_plastic_white"/>
            <geom type="box" pos="0.0 0.0 0.0" size="3.6 3.6 0.3"  material="pbr_plastic_gray"/>
            <geom type="box" pos="0.0 0.0 0.0" size="3.4 3.4 0.4"  material="pbr_plastic_white"/>
            <geom type="box" pos="0.0 0.0 0.0" size="3.2 3.2 0.5"  material="pbr_plastic_gray"/>
            <geom type="box" pos="0.0 0.0 0.0" size="3.0 3.0 0.6"  material="pbr_plastic_white"/>
            <geom type="box" pos="0.0 0.0 0.0" size="2.8 2.8 0.7"  material="pbr_plastic_gray"/>
            <geom type="box" pos="0.0 0.0 0.0" size="2.6 2.6 0.8"  material="pbr_plastic_white"/>
